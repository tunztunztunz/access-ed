{
  "name": "AccessEd",
  "description": "Tutoring and Private Instruction. Designed with your student at the center to assist them with accessing and navigating their education all while developing the skills they will carry far beyond the walls of a classroom.",
  "version": "1.0.0",
  "private": true,
  "author": "Dustin Simensen <dustinsimensen@gmail.com>",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf public",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "dev": "gatsby develop",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "start": "npm run dev",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc"
  },
  "eslintConfig": {
    "extends": [
      "wesbos",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:import/typescript"
    ],
    "rules": {
      "react/jsx-props-no-spreading": 0,
      "react/prop-types": 0,
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".tsx",
            ".ts"
          ]
        }
      ]
    }
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@reach/router": "^1.3.4",
    "@sanity/block-content-to-react": "^2.0.7",
    "babel-plugin-styled-components": "^1.12.0",
    "baseui": "^9.111.2",
    "classnames": "^2.2.6",
    "dotenv": "^8.2.0",
    "gatsby": "^3.1.1",
    "gatsby-plugin-advanced-sitemap": "^1.6.0",
    "gatsby-plugin-canonical-urls": "^3.1.0",
    "gatsby-plugin-create-client-paths": "^3.1.0",
    "gatsby-plugin-emotion": "^6.1.0",
    "gatsby-plugin-image": "^1.1.1",
    "gatsby-plugin-manifest": "^3.1.0",
    "gatsby-plugin-offline": "^4.1.0",
    "gatsby-plugin-react-helmet": "^4.1.0",
    "gatsby-plugin-robots-txt": "^1.5.5",
    "gatsby-plugin-sharp": "^3.1.1",
    "gatsby-plugin-styletron": "^6.1.0",
    "gatsby-plugin-typescript": "^3.1.0",
    "gatsby-source-filesystem": "^3.1.0",
    "gatsby-source-sanity": "^7.0.0",
    "gatsby-transformer-json": "^3.1.0",
    "gatsby-transformer-sharp": "^3.1.0",
    "modern-normalize": "^1.0.0",
    "normalize.css": "^8.0.1",
    "polished": "^4.1.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.2.0",
    "styletron-engine-atomic": "^1.4.7",
    "styletron-react": "^6.0.1",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@types/styletron-engine-atomic": "^1.1.0",
    "@types/styletron-react": "^5.0.2",
    "@types/styletron-standard": "^2.0.1",
    "@types/classnames": "^2.2.11",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-blvd": "^1.1.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-wesbos": "^1.0.1",
    "eslint-plugin-baseui": "^9.111.2",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^3.1.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2"
  }
}
